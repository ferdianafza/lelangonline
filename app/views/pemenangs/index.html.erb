<p id="notice"><%= notice %></p>

<h1>Pemenangs</h1>

<table>
  <thead>
    <tr>
      <th>Lelang</th>
      <th>User</th>
      <th>Harga</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pemenangs.each do |pemenang| %>
      <tr>
        <td><%= pemenang.lelang_id %></td>
        <td><%= pemenang.user_id %></td>
        <td><%= pemenang.harga %></td>
        <td><%= link_to 'Show', pemenang %></td>
        <td><%= link_to 'Edit', edit_pemenang_path(pemenang) %></td>
        <td><%= link_to 'Destroy', pemenang, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pemenang', new_pemenang_path %>
